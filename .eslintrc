{
    "extends": "airbnb",
    "settings": {
      "import/resolver": "webpack"
    },
    "globals": {
      "localStorage": true
    },
    "overrides": [
      {
        "files": [
          "storybook/**/*.js"
        ],
        "rules": {
          "import/no-extraneous-dependencies": "off",
          "react/jsx-filename-extension": "off",
          "indent": [
            "error",
            2
          ],
          "react/jsx-indent": [
            "error",
            2
          ],
          "react/jsx-indent-props": [
            "error",
            2
          ]
        }
      }
    ]
  }